{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "tauri_toolbar",
    "version": "0.0.0",
    "identifier": "com.tauri.dev",
    "build": {
        "beforeDevCommand": "pnpm dev",
        "devUrl": "http://localhost:1420",
        "beforeBuildCommand": "pnpm build",
        "frontendDist": "../dist"
    },
    "app": {
        "windows": [
            {
                "title": "tauri_toolbar",
                "width": 220,
                "maxWidth": 220,
                "minHeight": 320,
                "height": 320,
                "maxHeight": 760,
                "decorations": false,
                "alwaysOnTop": true,
                "transparent": true,
                "shadow": false,
                "resizable": false
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}